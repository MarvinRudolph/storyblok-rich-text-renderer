{
  "name": "@marvinrudolph/vue-storyblok-rich-text-renderer",
  "version": "1.1.4",
  "description": "Vue.js plugin for Storyblok's rich-text renderer",
  "main": "dist/vue-storyblok-rich-text-renderer.common.js",
  "types": "dist/plugin/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "yarn test:unit && vue-cli-service build --target lib --name vue-storyblok-rich-text-renderer src/plugin/index.ts",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarvinRudolph/storyblok-rich-text-renderer.git"
  },
  "author": {
    "name": "Marvin Rudolph",
    "email": "info@marvin-rudolph.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarvinRudolph/storyblok-rich-text-renderer/issues"
  },
  "homepage": "https://storyblok-rich-text-renderer.netlify.com/vue-plugin",
  "dependencies": {
    "@marvinrudolph/storyblok-rich-text-types": "^1.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint-plugin-vue": "^6.1.2",
    "storyblok-js-client": "^2.0.12",
    "vue-template-compiler": "^2.6.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
